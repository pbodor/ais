{
  "name": "ais-js",
  "version": "0.1.3",
  "description": "AIS reader for Javascript/Typescript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p ./src/tsconfig.json",
    "test": "karma start",
    "doc": "typedoc --name \"ais-0.1.0 API Documentation\" --mode modules --exclude */**/*.spec.ts --exclude */**/index.ts --readme README.md --module commonjs --target ES5 --out doc src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyosho-/ais.git"
  },
  "keywords": [
    "AIS"
  ],
  "author": "kyosho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kyosho-/ais/issues"
  },
  "homepage": "https://github.com/kyosho-/ais#readme",
  "devDependencies": {
    "@types/jasmine": "^2.5.43",
    "@types/node": "^7.0.5",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-typescript": "^2.1.7",
    "typedoc": "^0.5.7",
    "typescript": "^2.2.0"
  }
}
